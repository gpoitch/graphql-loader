{
  "name": "webpack-graphql-loader",
  "version": "1.0.2",
  "description": "webpack graphql loader",
  "main": "./lib/loader.js",
  "types": "./lib/loader.d.ts",
  "author": "Stephen Wan <stephen@stephenwan.com>",
  "homepage": "https://github.com/stephen/graphql-loader/",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephen/graphql-loader.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm test && npm run build",
    "test": "jest --testEnvironment node"
  },
  "devDependencies": {
    "@babel/types": "^7.10.4",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.4",
    "@types/loader-utils": "^2.0.1",
    "@types/memory-fs": "^0.3.0",
    "@types/node": "^14.0.20",
    "@types/webpack": "^4.41.21",
    "graphql": "^15.3.0",
    "jest": "^26.1.0",
    "memory-fs": "^0.5.0",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "loader-utils": "^2.0.0",
    "pify": "^5.0.0"
  },
  "peerDependencies": {
    "graphql": ">=15.0.0",
    "ts-loader": "^4.4.1"
  },
  "jest": {
    "testMatch": [
      "**/test/?(*.)(spec).ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    }
  }
}
